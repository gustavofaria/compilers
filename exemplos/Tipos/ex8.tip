programa
   a, b, soma: inteiro;
   
funcao multiplicador(m:inteiro, x:inteiro):inteiro
z: inteiro;
inicio
  z := 5;
  z := z * m * x;
  retorne z;
fim;


funcao somador(x: inteiro, y: inteiro, z: inteiro) : inteiro
r: inteiro;
m: inteiro;
inicio
    m := 1;
    r := multiplicador(m, x + y + z);
    retorne r;
fim;

  
inicio
   a := 2;
   b := 3;
   soma := somador(a+1, b+2, 5);
   saida a, b, soma;
fim;


